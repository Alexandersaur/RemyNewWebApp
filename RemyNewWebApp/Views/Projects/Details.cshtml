                                <!--begin::Status-->
                                <div class="d-flex align-items-center mb-1">
                                    <a href="#" class="text-gray-800 text-hover-primary fs-2 fw-bolder me-3">@Model.Name</a>
                                    @if (Model.ProjectPriorityId == 4)
                                    {
                                        <span class="badge badge-light-primary me-auto">@Model.ProjectPriority.Name</span>
                                    }
                                    @if (Model.ProjectPriorityId == 3)
                                    {
                                        <span class="badge badge-light-success me-auto">@Model.ProjectPriority.Name</span>
                                    }
                                    @if (Model.ProjectPriorityId == 2)
                                    {
                                        <span class="badge badge-light-warning me-auto">@Model.ProjectPriority.Name</span>
                                    }
                                    @if (Model.ProjectPriorityId == 1)
                                    {
                                        <span class="badge badge-light-danger me-auto">@Model.ProjectPriority.Name</span>
                                    }
                                </div>
